{
  "name": "database-service",
  "version": "1.0.0",
  "description": "A REST API and unit tests for interfacing with mongo db",
  "main": "dest/server.js",
  "scripts": {
    "format": "npx prettier --write .",
    "lint": "npx eslint .",
    "compile": "npx tsc",
    "compile-test": "npx tsc -b",
    "build": "npm run format && npm run lint && npm run compile",
    "prestart": "npm run build",
    "start": "node dest/server.js",
    "dev": "concurrently 'npx tsc --watch' 'npx nodemon'",
    "pretest": "npm run format && npm run lint && npm run compile-test",
    "test": "npx mocha dest/test"
  },
  "author": "Tom Riley",
  "license": "ISC",
  "dependencies": {
    "@azure/identity": "^1.5.2",
    "express": "^4.17.1",
    "mongodb": "^4.0.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
